{
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "strict": true,
    "noImplicitAny": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": [
    "src",
    "./.d.ts"
  ]    
}